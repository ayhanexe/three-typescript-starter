{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "paths": {
      "@sass/*": ["src/sass/*"],
      "@core/*": ["src/core/*"]
    }
  }
}
